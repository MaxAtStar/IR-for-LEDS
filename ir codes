#Codes are in order from LEFT TO RIGHT. It skips the DIY codes. This is for a 27(?) pin remote. 

/*
 * IRremote: IRsendDemo - demonstrates sending IR codes with IRsend
 * An IR LED must be connected to Arduino PWM pin 3.
 * Version 0.1 July, 2009
 * Copyright 2009 Ken Shirriff
 * http://arcfn.com
 */

#include <IRremote.h>

IRsend irsend;

void setup()
{
  Serial.begin(9600);
}

void loop() {
  //if (Serial.read() != -1) {
//   for (int i = 0; i < 3; i++) {
unsigned int brightnessup[68] = {9200,4400,650,500,550,600,550,550,600,550,550,550,600,550,600,500,600,550,600,1650,550,1700,600,1650,600,1650,550,1650,650,1600,600,1650,550,1700,600,550,550,550,600,1650,550,1700,600,1650,600,500,600,1700,550,550,600,1650,550,1700,500,600,600,550,600,500,600,1650,600,550,550,1700,550,};
irsend.sendRaw(brightnessup, 68,38); // Sony TV power code
      delay(100); 
irsend.sendRaw(brightnessup, 68,38); // Sony TV power code
      delay(100);
irsend.sendRaw(brightnessup, 68,38); // Sony TV power code
      delay(100);            
      unsigned int blue[68] = {9100,4450,600,550,550,550,600,550,600,500,550,600,600,500,600,550,550,550,600,1650,550,1700,600,1650,600,1650,550,1650,600,1650,600,1650,550,1700,600,1650,600,500,600,1650,600,550,600,500,600,550,600,1650,550,550,600,500,650,1650,600,500,600,1650,600,1650,550,1650,650,500,550,1700,600,};
      irsend.sendRaw(blue, 68,38); // Sony TV power code
      delay(1000);
unsigned int lgreen[68] = {9150,4400,650,500,550,600,600,500,600,550,550,550,600,550,600,500,600,550,600,1650,550,1650,650,1650,550,1650,550,1700,600,1650,600,1650,550,1700,600,500,550,600,550,550,650,1600,600,1650,550,600,550,550,600,550,550,1650,650,1600,600,1650,550,600,600,500,650,1600,600,1650,550,1700,600,};      
      irsend.sendRaw(lgreen, 68,38); // Sony TV power code
      delay(1000); 
unsigned int brightnessdown[68] = {9150,4450,600,500,600,550,600,500,650,500,550,600,550,550,650,500,500,650,550,1650,550,1700,600,1650,600,1650,550,1700,600,1650,550,1650,600,1650,650,1600,600,550,600,1650,600,1650,550,1650,650,500,550,1700,600,500,600,550,600,1650,550,600,550,550,600,550,550,1650,650,500,550,1700,600,};
irsend.sendRaw(brightnessdown, 68,38); // Sony TV power code
      delay(100);      
irsend.sendRaw(brightnessdown, 68,38); // Sony TV power code
      delay(100);    
irsend.sendRaw(brightnessdown, 68,38); // Sony TV power code
      delay(100); 
      irsend.sendRaw(blue, 68,38); // Sony TV power code
      delay(1000);        
      irsend.sendRaw(lgreen, 68,38); // Sony TV power code
      delay(1000);        
 
    //}
//  }
}
